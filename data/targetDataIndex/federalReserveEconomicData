   "source": [
    "\"\"\"\n",
    "fred_vecm_full.py  —  End-to-end VECM estimation using FRED data\n",
    "Recreates the MATLAB Econometrics Toolbox example in one Python file.\n",
    "\"\"\"\n",
    "\n",
    "import os\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.stats import chi2\n",
    "from datetime import datetime\n",
    "\n",
    "# optional dependencies\n",
    "try:\n",
    "    from fredapi import Fred\n",
    "    from statsmodels.tsa.vector_ar.vecm import coint_johansen, VECM\n",
    "except ImportError:\n",
    "    Fred = None\n",
    "    coint_johansen = None\n",
    "    VECM = None"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 131,
   "id": "2888293d",
   "metadata": {},
   "outputs": [],
   "source": [
    "SERIES = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\", \"USREC\"]\n",
    "\n",
    "plt.rcParams[\"figure.figsize\"] = (11, 6)\n",
    "pd.options.display.max_columns = 20"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 132,
   "id": "6ee0c61e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ---------------------------------------------------------------------\n",
    "#  Data fetch and transformation\n",
    "# ---------------------------------------------------------------------\n",
    "def fetch_fred_data():\n",
    "    if Fred is None or FRED_API_KEY is None:\n",
    "        raise RuntimeError(\"fredapi not installed or FRED_API_KEY not set\")\n",
    "    fred = Fred(api_key=FRED_API_KEY)\n",
    "    data = {s: fred.get_series(s) for s in SERIES}\n",
    "    df = pd.concat(data, axis=1)\n",
    "    df.index.name = \"Date\"\n",
    "    return df\n",
    "\n",
    "\n",
    "def transform_data(df):\n",
    "    \"\"\"100*log for all except FEDFUNDS & USREC\"\"\"\n",
    "    df2 = df.copy()\n",
    "    to_transform = [c for c in df2.columns if c not in [\"FEDFUNDS\", \"USREC\"]]\n",
    "    df2[to_transform] = np.log(df2[to_transform]) * 100\n",
    "    return df2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 133,
   "id": "21922afa",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ---------------------------------------------------------------------\n",
    "#  Statistical tests\n",
    "# ---------------------------------------------------------------------\n",
    "def johansen_compare(df, lags_diff=1):\n",
    "    \"\"\"Run Johansen trace tests for H* (trend) and H1 (constant).\"\"\"\n",
    "    df = df.dropna().astype(float)\n",
    "    if np.any(np.isinf(df.values)):\n",
    "        raise ValueError(\"Input data contains inf values\")\n",
    "    df = (df - df.mean()) / df.std()        # stabilize scale\n",
    "\n",
    "    try:\n",
    "        res_Hstar = coint_johansen(df.values, det_order=2, k_ar_diff=lags_diff)\n",
    "        res_H1 = coint_johansen(df.values, det_order=1, k_ar_diff=lags_diff)\n",
    "    except np.linalg.LinAlgError as e:\n",
    "        print(\"Johansen test failed:\", e, \"→ retrying with smaller lag order…\")\n",
    "        res_Hstar = coint_johansen(df.values, det_order=2, k_ar_diff=max(lags_diff-1, 0))\n",
    "        res_H1 = coint_johansen(df.values, det_order=1, k_ar_diff=max(lags_diff-1, 0))\n",
    "\n",
    "    def table(res):\n",
    "        crit95 = res.cvt[:, 1] if hasattr(res, \"cvt\") else np.nan\n",
    "        return pd.DataFrame({\n",
    "            \"r\": range(len(res.lr1)),\n",
    "            \"trace_stat\": res.lr1,\n",
    "            \"crit_95\": crit95,\n",
    "            \"eigval\": res.eig\n",
    "        })\n",
    "    return table(res_Hstar), table(res_H1)\n",
    "\n",
    "\n",
    "def lr_test_between_models(df, r, P=2):\n",
    "    \"\"\"Likelihood-ratio comparison: H* (lt) vs H1 (co).\"\"\"\n",
    "    vecm_lt = VECM(df, k_ar_diff=P-1, coint_rank=r, deterministic=\"lt\").fit()\n",
    "    vecm_co = VECM(df, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "    stat = 2 * (vecm_lt.llf - vecm_co.llf)\n",
    "    p_value = 1 - chi2.cdf(stat, r)\n",
    "    crit_95 = chi2.ppf(0.95, r)\n",
    "    return {\"stat\": stat, \"p\": p_value, \"crit_95\": crit_95,\n",
    "            \"vecm_lt\": vecm_lt, \"vecm_co\": vecm_co}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 134,
   "id": "98596915",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ---------------------------------------------------------------------\n",
    "#  Estimation & constraints\n",
    "# ---------------------------------------------------------------------\n",
    "def fit_vecm(df, r, P=2, deterministic=\"co\"):\n",
    "    return VECM(df, k_ar_diff=P-1, coint_rank=r, deterministic=deterministic).fit()\n",
    "\n",
    "\n",
    "def constrain_tvalues(vecm_res, threshold=2):\n",
    "    alpha, gamma = vecm_res.alpha.copy(), vecm_res.gamma.copy()\n",
    "    t_alpha, t_gamma = vecm_res.tvalues_alpha, vecm_res.tvalues_gamma\n",
    "    alpha[np.abs(t_alpha) < threshold] = 0\n",
    "    gamma[np.abs(t_gamma) < threshold] = 0\n",
    "    return alpha, gamma\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 135,
   "id": "cc383975",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ---------------------------------------------------------------------\n",
    "#  Plotting\n",
    "# ---------------------------------------------------------------------\n",
    "def shade_recessions(ax, rec_series, color=\"lightgray\", alpha=0.4):\n",
    "    rec = rec_series.dropna()\n",
    "    in_rec, start = False, None\n",
    "    for dt, val in rec.items():\n",
    "        if val and not in_rec:\n",
    "            in_rec, start = True, dt\n",
    "        elif not val and in_rec:\n",
    "            ax.axvspan(start, dt, color=color, alpha=alpha)\n",
    "            in_rec = False\n",
    "    if in_rec and start:\n",
    "        ax.axvspan(start, rec.index[-1], color=color, alpha=alpha)\n",
    "\n",
    "\n",
    "def plot_cointegrating_relations(vecm_res, df, P=2):\n",
    "    \"\"\"\n",
    "    Plot cointegrating relations for fitted VECM (handles dimensionality correctly).\n",
    "    \"\"\"\n",
    "    beta = vecm_res.beta               # shape (r, n)\n",
    "    n, r = beta.shape[1], beta.shape[0]\n",
    "\n",
    "    # constants / trend coefficients associated with cointegration\n",
    "    beta_const = getattr(vecm_res, \"det_coef_coint\", np.zeros((1, r)))  # (1, r)\n",
    "    beta_trend = np.zeros((1, r))  # no trend in H1 model\n",
    "\n",
    "    Y = df.values                   # (T, n)\n",
    "    t = np.arange(-(P - 1), len(df) - P).reshape(-1, 1)  # (T, 1)\n",
    "    ones = np.ones((len(df), 1))\n",
    "\n",
    "    # Compute cointegrating relations properly: Y @ beta.T + 1*c0 + t*d0\n",
    "    cr = Y @ beta.T + ones @ beta_const + t @ beta_trend\n",
    "\n",
    "    # Plot\n",
    "    fig, ax = plt.subplots(figsize=(11, 6))\n",
    "    for i in range(r):\n",
    "        ax.plot(df.index, cr[:, i], label=f\"Relation {i+1}\")\n",
    "    ax.set_title(\"Cointegrating Relations\")\n",
    "    ax.legend(frameon=False)\n",
    "    plt.show()"
   ]
  },
