  {
   "cell_type": "code",
   "execution_count": 144,
   "id": "9d25609f",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def subsample_sensitivity_analysis(full_data, r=4, P=2):\n",
    "    \"\"\"\n",
    "    Counterfactual experiment: Great Inflation vs. Great Moderation\n",
    "    (works even when vecm_to_var() is unavailable)\n",
    "    \"\"\"\n",
    "    cols = [\"GDP\",\"GDPDEF\",\"COE\",\"HOANBS\",\"PCEC\",\"GPDI\",\"FEDFUNDS\"]\n",
    "    df = full_data[cols].copy().dropna()\n",
    "    df = (df - df.mean()) / df.std()\n",
    "\n",
    "    # --- define subsamples ---\n",
    "    t1 = pd.Timestamp(\"1979-06-30\")   # Great Inflation ends\n",
    "    t2 = pd.Timestamp(\"1984-03-31\")   # Great Moderation begins\n",
    "    Y1, Y2 = df.loc[:t1], df.loc[t2:]\n",
    "\n",
    "    print(f\"Great Inflation: {Y1.index[0].date()} → {Y1.index[-1].date()} ({len(Y1)} quarters)\")\n",
    "    print(f\"Great Moderation: {Y2.index[0].date()} → {Y2.index[-1].date()} ({len(Y2)} quarters)\")\n",
    "\n",
    "    # --- fit VECMs ---\n",
    "    vecm_GI = VECM(Y1, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "    vecm_GM = VECM(Y2, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "\n",
    "    # --- residuals from Great Inflation ---\n",
    "    E1 = vecm_GI.resid              # (T₁ × n)\n",
    "    n_obs, n_vars = E1.shape\n",
    "\n",
    "    # --- convert Great-Moderation VECM to VAR manually ---\n",
    "    # VAR(p) representation:  ΔYₜ = ΠYₜ₋₁ + ΣΓᵢΔYₜ₋ᵢ + c + εₜ\n",
    "    # ⇒  Yₜ = A₁Yₜ₋₁ + ... + AₚYₜ₋ₚ + c + εₜ\n",
    "    A_mats = []                     # store A₁…Aₚ\n",
    "    I = np.eye(n_vars)\n",
    "    Pi = vecm_GM.alpha @ vecm_GM.beta.T          # long-run term\n",
    "    Gamma = vecm_GM.gamma                        # list of short-run matrices\n",
    "    A1 = I + Pi + (Gamma[0] if len(Gamma) else 0)\n",
    "    A_mats.append(A1)\n",
    "    for i in range(1, P-1):\n",
    "        Ai = (Gamma[i] if i < len(Gamma) else 0)\n",
    "        A_mats.append(Ai)\n",
    "    c = vecm_GM.det_coef.reshape(-1) if hasattr(vecm_GM, \"det_coef\") else np.zeros(n_vars)\n",
    "\n",
    "    # --- initialize with first P observations of Great Moderation ---\n",
    "    Y0 = Y2.iloc[:P].values\n",
    "    YY = []\n",
    "    hist = list(Y0.copy())\n",
    "\n",
    "    # --- propagate Great-Inflation residuals through GM dynamics ---\n",
    "    for t in range(n_obs):\n",
    "        y_pred = c.copy()\n",
    "        for lag, A in enumerate(A_mats):\n",
    "            y_pred += A @ hist[-(lag+1)]\n",
    "        y_new = y_pred + E1[t]\n",
    "        YY.append(y_new)\n",
    "        hist.append(y_new)\n",
    "    YY = np.array(YY)\n",
    "\n",
    "    # --- align with dates ---\n",
    "    Y2_aligned = Y2.iloc[P:P+n_obs]\n",
    "    YY_df = pd.DataFrame(YY, index=Y2_aligned.index, columns=Y2.columns)\n",
    "\n",
    "    # --- compute real GDP (GDP − GDPDEF) ---\n",
    "    real_actual = Y2_aligned[\"GDP\"] - Y2_aligned[\"GDPDEF\"]\n",
    "    real_counter = YY_df[\"GDP\"] - YY_df[\"GDPDEF\"]\n",
    "\n",
    "    # --- plot ---\n",
    "    plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "    fig, ax = plt.subplots(figsize=(9,5))\n",
    "    ax.plot(real_actual.index, real_actual, \"b\", lw=1.8,\n",
    "            label=\"Reported Great Moderation Data\")\n",
    "    ax.plot(real_counter.index, real_counter, \"r\", lw=1.4,\n",
    "            label=\"Filtered Great Inflation Residuals\")\n",
    "    ax.set_title(\"Real GDP Sub-Sample Comparison: Great Inflation vs Great Moderation\")\n",
    "    ax.set_ylabel(\"Normalized Real GDP (Scaled)\")\n",
    "    ax.legend(frameon=False)\n",
    "    ax.grid(True, alpha=0.3)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    return {\"GreatInflation_VECM\": vecm_GI,\n",
    "            \"GreatModeration_VECM\": vecm_GM,\n",
    "            \"FilteredSeries\": YY_df,\n",
    "            \"ActualSeries\": Y2_aligned}\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ccd1271b",
   "metadata": {},
   "outputs": [
