   "source": [
    "#!/usr/bin/env python3\n",
    "\"\"\"\n",
    "VECMAnalyzer — FRED data, Johansen tests, and VECM estimation\n",
    "All logic wrapped in a single static-method class.\n",
    "\"\"\"\n",
    "\n",
    "import os\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.stats import chi2\n",
    "\n",
    "try:\n",
    "    from fredapi import Fred\n",
    "    from statsmodels.tsa.vector_ar.vecm import coint_johansen, VECM\n",
    "except ImportError:\n",
    "    Fred = None\n",
    "    coint_johansen = None\n",
    "    VECM = None\n",
    "\n",
    "\n",
    "class VECMAnalyzer:\n",
    "    \"\"\"Encapsulated static-method pipeline for FRED VECM estimation.\"\"\"\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    # CONFIG\n",
    "    FRED_API_KEY = os.getenv(\"FRED_API_KEY\")\n",
    "    SERIES = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\", \"USREC\"]\n",
    "    plt.rcParams[\"figure.figsize\"] = (11, 6)\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def fetch_fred_data():\n",
    "        if Fred is None or VECMAnalyzer.FRED_API_KEY is None:\n",
    "            raise RuntimeError(\"fredapi not installed or FRED_API_KEY not set.\")\n",
    "        fred = Fred(api_key=VECMAnalyzer.FRED_API_KEY)\n",
    "        data = {s: fred.get_series(s) for s in VECMAnalyzer.SERIES}\n",
    "        df = pd.concat(data, axis=1)\n",
    "        df.index.name = \"Date\"\n",
    "        return df\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def transform_data(df: pd.DataFrame) -> pd.DataFrame:\n",
    "        df2 = df.copy()\n",
    "        to_transform = [c for c in df2.columns if c not in [\"FEDFUNDS\", \"USREC\"]]\n",
    "        df2[to_transform] = np.log(df2[to_transform]) * 100\n",
    "        return df2\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def johansen_compare(df: pd.DataFrame, lags_diff: int = 1):\n",
    "        df = df.dropna().astype(float)\n",
    "        df = (df - df.mean()) / df.std()\n",
    "        try:\n",
    "            res_Hstar = coint_johansen(df.values, det_order=2, k_ar_diff=lags_diff)\n",
    "            res_H1 = coint_johansen(df.values, det_order=1, k_ar_diff=lags_diff)\n",
    "        except np.linalg.LinAlgError as e:\n",
    "            print(\"Johansen test failed:\", e, \"→ retrying with smaller lag order…\")\n",
    "            res_Hstar = coint_johansen(df.values, det_order=2, k_ar_diff=max(lags_diff - 1, 0))\n",
    "            res_H1 = coint_johansen(df.values, det_order=1, k_ar_diff=max(lags_diff - 1, 0))\n",
    "\n",
    "        def table(res):\n",
    "            crit95 = res.cvt[:, 1] if hasattr(res, \"cvt\") else np.nan\n",
    "            return pd.DataFrame({\n",
    "                \"r\": range(len(res.lr1)),\n",
    "                \"trace_stat\": res.lr1,\n",
    "                \"crit_95\": crit95,\n",
    "                \"eigval\": res.eig\n",
    "            })\n",
    "        return table(res_Hstar), table(res_H1)\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def lr_test_between_models(df: pd.DataFrame, r: int, P: int = 2):\n",
    "        vecm_lt = VECM(df, k_ar_diff=P - 1, coint_rank=r, deterministic=\"lt\").fit()\n",
    "        vecm_co = VECM(df, k_ar_diff=P - 1, coint_rank=r, deterministic=\"co\").fit()\n",
    "        stat = 2 * (vecm_lt.llf - vecm_co.llf)\n",
    "        p_value = 1 - chi2.cdf(stat, r)\n",
    "        crit_95 = chi2.ppf(0.95, r)\n",
    "        return {\"stat\": stat, \"p\": p_value, \"crit_95\": crit_95, \"vecm_lt\": vecm_lt, \"vecm_co\": vecm_co}\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def fit_vecm(df: pd.DataFrame, r: int, P: int = 2, deterministic: str = \"co\"):\n",
    "        return VECM(df, k_ar_diff=P - 1, coint_rank=r, deterministic=deterministic).fit()\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def constrain_tvalues(vecm_res, threshold: float = 2.0):\n",
    "        alpha, gamma = vecm_res.alpha.copy(), vecm_res.gamma.copy()\n",
    "        t_alpha, t_gamma = vecm_res.tvalues_alpha, vecm_res.tvalues_gamma\n",
    "        alpha[np.abs(t_alpha) < threshold] = 0\n",
    "        gamma[np.abs(t_gamma) < threshold] = 0\n",
    "        return alpha, gamma\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def shade_recessions(ax, rec_series: pd.Series, color=\"lightgray\", alpha=0.5):\n",
    "        \"\"\"Draw gray NBER recession bars based on the USREC indicator.\"\"\"\n",
    "        rec = rec_series.dropna()\n",
    "        in_rec, start = False, None\n",
    "        for date, val in rec.items():\n",
    "            if val and not in_rec:\n",
    "                in_rec, start = True, date\n",
    "            elif not val and in_rec:\n",
    "                ax.axvspan(start, date, color=color, alpha=alpha, lw=0)\n",
    "                in_rec = False\n",
    "        if in_rec and start:\n",
    "            ax.axvspan(start, rec.index[-1], color=color, alpha=alpha, lw=0)\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def plot_cointegrating_relations(vecm_res, df: pd.DataFrame, P: int = 2, rec_series: pd.Series | None = None):\n",
    "        \"\"\"\n",
    "        Plot cointegrating relations in a MATLAB-like style:\n",
    "        - Uses statsmodels beta orientation (n_endog × r)\n",
    "        - relation_i(t) = Y(t,:) @ beta[:,i] + const_i + trend_i * t\n",
    "        - Normalizes each relation (mean=0, std=1) for common vertical scale\n",
    "        - Optional NBER recession shading via USREC\n",
    "        \"\"\"\n",
    "        beta = vecm_res.beta            # (n_endog, r)\n",
    "        n, r = beta.shape\n",
    "\n",
    "        beta_const = getattr(vecm_res, \"det_coef_coint\", None)\n",
    "        if beta_const is None or beta_const.size == 0:\n",
    "            beta_const = np.zeros((1, r))\n",
    "        beta_trend = np.zeros((1, r))   # no trend in H1\n",
    "\n",
    "        Y = df.values                   # (T, n_endog)\n",
    "        T = len(df)\n",
    "        t = np.arange(T).reshape(-1, 1)\n",
    "        ones = np.ones((T, 1))\n",
    "\n",
    "        # Compute and normalize relations\n",
    "        cr = Y @ beta + ones @ beta_const + t @ beta_trend     # (T, r)\n",
    "        cr = (cr - cr.mean(axis=0)) / cr.std(axis=0) * 5       # ~±10 visually\n",
    "\n",
    "        plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "        fig, ax = plt.subplots(figsize=(10, 5))\n",
    "        for i in range(r):\n",
    "            ax.plot(df.index, cr[:, i], label=f\"Relation {i+1}\", lw=1.5)\n",
    "\n",
    "        if rec_series is not None:\n",
    "            VECMAnalyzer.shade_recessions(ax, rec_series)\n",
    "\n",
    "        ax.set_title(\"Cointegrating Relations\", fontsize=12)\n",
    "        ax.set_ylabel(\"Normalized Level\")\n",
    "        ax.legend(frameon=False, loc=\"upper right\")\n",
    "        ax.grid(True, alpha=0.3)\n",
    "        ax.set_xlim(df.index[0], df.index[-1])\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    @staticmethod\n",
    "    def main():\n",
    "        print(\"Fetching FRED data…\")\n",
    "        fred_df = VECMAnalyzer.fetch_fred_data()\n",
    "        data = VECMAnalyzer.transform_data(fred_df)\n",
    "        print(\"Data shape:\", data.shape)\n",
    "\n",
    "        cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]\n",
    "        data_sub = data[cols].dropna().loc[\"1960\":\"2020\"]\n",
    "        data_sub = (data_sub - data_sub.mean()) / data_sub.std()\n",
    "\n",
    "        print(\"\\nRunning Johansen trace tests…\")\n",
    "        tHstar, tH1 = VECMAnalyzer.johansen_compare(data_sub)\n",
    "        print(\"\\nH* (trend):\\n\", tHstar)\n",
    "        print(\"\\nH1 (constant):\\n\", tH1)\n",
    "\n",
    "        r, P = 4, 2\n",
    "        print(\"\\nLikelihood-ratio test (H* vs H1)…\")\n",
    "        lr = VECMAnalyzer.lr_test_between_models(data_sub, r, P)\n",
    "        print(lr)\n",
    "\n",
    "        print(\"\\nFitting baseline VECM (H1)…\")\n",
    "        vecm_res = VECMAnalyzer.fit_vecm(data_sub, r, P)\n",
    "        alpha_c, gamma_c = VECMAnalyzer.constrain_tvalues(vecm_res)\n",
    "        print(\"Alpha (adjustment) constrained:\\n\", alpha_c)\n",
    "        print(\"Gamma (short-run) constrained:\\n\", gamma_c)\n",
    "\n",
    "        print(\"\\nPlotting cointegrating relations…\")\n",
    "        # Pass USREC aligned to the same index\n",
    "        rec_aligned = data.loc[data_sub.index, \"USREC\"].astype(float) if \"USREC\" in data.columns else None\n",
    "        VECMAnalyzer.plot_cointegrating_relations(vecm_res, data_sub, P=P, rec_series=rec_aligned)\n",
    "        print(\"\\nDone.\")\n",
    "\n",
    "\n",
    "# ------------------------------------------------------------------\n",
    "if __name__ == \"__main__\":\n",
    "    VECMAnalyzer.main()\n"
   ]
  },
