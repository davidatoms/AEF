   "cell_type": "code",
   "execution_count": 140,
   "id": "13af8a87",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def analyze_recessions_individually(full_data, r=4, P=2, fh=12):\n",
    "    \"\"\"\n",
    "    Produce individual 12-quarter VECM Real GDP forecasts before each NBER recession since 1990.\n",
    "    \n",
    "    full_data : DataFrame\n",
    "        Quarterly, transformed dataset (from VECMAnalyzer.transform_data + resample('Q')).\n",
    "    r : int\n",
    "        Cointegration rank.\n",
    "    P : int\n",
    "        Lag order in levels (so k_ar_diff = P - 1).\n",
    "    fh : int\n",
    "        Forecast horizon (quarters ahead).\n",
    "    \"\"\"\n",
    "\n",
    "    # --- required variables ---\n",
    "    cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\", \"USREC\"]\n",
    "    df = full_data[cols].copy().dropna()\n",
    "    df = df.loc[\"1990\":]\n",
    "    df_std = (df - df.mean()) / df.std()\n",
    "\n",
    "    rec_series = df[\"USREC\"].astype(int)\n",
    "\n",
    "    # --- identify start of each recession ---\n",
    "    recession_starts = []\n",
    "    prev = 0\n",
    "    for date, val in rec_series.items():\n",
    "        if val == 1 and prev == 0:\n",
    "            recession_starts.append(date)\n",
    "        prev = val\n",
    "\n",
    "    # --- de-duplicate short gaps (e.g., quarterly overlaps) ---\n",
    "    starts_clean = []\n",
    "    for i, d in enumerate(recession_starts):\n",
    "        if i == 0 or (d - recession_starts[i - 1]).days > 120:\n",
    "            starts_clean.append(d)\n",
    "\n",
    "    recession_starts = starts_clean\n",
    "    print(\"Recession starts identified since 1990:\")\n",
    "    for d in recession_starts:\n",
    "        print(\" \", d.date())\n",
    "\n",
    "    # --- iterate through recessions ---\n",
    "    for start in recession_starts:\n",
    "        # one quarter before the recession\n",
    "        origin_idx = df_std.index.get_indexer([start], method=\"nearest\")[0] - 1\n",
    "        if origin_idx < 40:\n",
    "            continue\n",
    "        origin_date = df_std.index[origin_idx]\n",
    "        sample = df_std.iloc[: origin_idx + 1]\n",
    "\n",
    "        # fit and forecast\n",
    "        vecm = VECM(sample.iloc[:, :-1], k_ar_diff=P - 1, coint_rank=r, deterministic=\"co\").fit()\n",
    "        fc = vecm.predict(steps=fh)\n",
    "        real_gdp_forecast = fc[:, 0] - fc[:, 1]\n",
    "\n",
    "        # actuals\n",
    "        forecast_dates = pd.date_range(start=origin_date, periods=fh, freq=\"Q\") + pd.offsets.QuarterEnd(0)\n",
    "        actual_real_gdp = (\n",
    "            df_std[\"GDP\"].reindex(forecast_dates, method=\"nearest\") -\n",
    "            df_std[\"GDPDEF\"].reindex(forecast_dates, method=\"nearest\")\n",
    "        )\n",
    "\n",
    "        # ±1σ confidence band (roughly from residual covariance)\n",
    "        cov_u = vecm.sigma_u\n",
    "        sigma = np.sqrt(cov_u[0, 0] - 2 * cov_u[0, 1] + cov_u[1, 1])\n",
    "\n",
    "        # plot\n",
    "        plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "        fig, ax = plt.subplots(figsize=(9, 5))\n",
    "        ax.plot(forecast_dates, real_gdp_forecast + sigma, \"--r\", lw=1.2, label=\"Forecast +1σ\")\n",
    "        ax.plot(forecast_dates, real_gdp_forecast, \"r\", lw=1.8, label=\"Forecast\")\n",
    "        ax.plot(forecast_dates, real_gdp_forecast - sigma, \"--r\", lw=1.2, label=\"Forecast -1σ\")\n",
    "        ax.plot(forecast_dates, actual_real_gdp, \"b\", lw=1.5, label=\"Actual\")\n",
    "\n",
    "        ax.set_title(f\"Real GDP vs. 12-Quarter Forecast — Origin: {origin_date.date()}\")\n",
    "        ax.set_ylabel(\"Normalized Real GDP (Scaled)\")\n",
    "        ax.legend(frameon=False, loc=\"best\")\n",
    "        ax.grid(True, alpha=0.3)\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n"
   ]
  },
