   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def analyze_all_recessions(full_data, r=4, P=2, fh=12):\n",
    "    \"\"\"\n",
    "    Perform 12-quarter VECM forecasts of Real GDP for all recessions since 1990.\n",
    "    \n",
    "    full_data : DataFrame\n",
    "        Quarterly, transformed dataset (from VECMAnalyzer.transform_data + resample('Q')).\n",
    "    r : int\n",
    "        Cointegration rank.\n",
    "    P : int\n",
    "        Lag order in levels (so k_ar_diff = P - 1).\n",
    "    fh : int\n",
    "        Forecast horizon (quarters ahead).\n",
    "    \"\"\"\n",
    "\n",
    "    # --- ensure required columns ---\n",
    "    cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\", \"USREC\"]\n",
    "    df = full_data[cols].copy().dropna()\n",
    "    df = df.loc[\"1990\":]                      # only use data from 1990 onward\n",
    "    df_q = (df - df.mean()) / df.std()        # same normalization\n",
    "\n",
    "    rec_series = df[\"USREC\"].astype(int)\n",
    "    rec_periods = rec_series[(rec_series == 1)].index\n",
    "\n",
    "    # --- find recession start dates (first quarter where USREC=1 after a 0) ---\n",
    "    recession_starts = []\n",
    "    prev = 0\n",
    "    for date, val in rec_series.items():\n",
    "        if val == 1 and prev == 0:\n",
    "            recession_starts.append(date)\n",
    "        prev = val\n",
    "\n",
    "    # --- remove any overlapping/short gaps ---\n",
    "    clean_starts = []\n",
    "    for i, d in enumerate(recession_starts):\n",
    "        if i == 0 or (d - recession_starts[i - 1]).days > 120:\n",
    "            clean_starts.append(d)\n",
    "    recession_starts = clean_starts\n",
    "\n",
    "    print(\"Detected recession starts since 1990:\")\n",
    "    for d in recession_starts:\n",
    "        print(\"  \", d.date())\n",
    "\n",
    "    # --- main analysis loop ---\n",
    "    results = {}\n",
    "    fig, ax = plt.subplots(figsize=(10, 6))\n",
    "    plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "\n",
    "    for start in recession_starts:\n",
    "        # Forecast origin = one quarter before the recession begins\n",
    "        origin_idx = df_q.index.get_indexer([start], method=\"nearest\")[0] - 1\n",
    "        if origin_idx < 40:  # ensure we have at least 10 years of history\n",
    "            continue\n",
    "        origin_date = df_q.index[origin_idx]\n",
    "        sample = df_q.iloc[: origin_idx + 1]\n",
    "\n",
    "        # --- fit and forecast 12 quarters ahead ---\n",
    "        vecm = VECM(sample.iloc[:, :-1], k_ar_diff=P - 1, coint_rank=r, deterministic=\"co\").fit()\n",
    "        fc = vecm.predict(steps=fh)\n",
    "        real_gdp_forecast = fc[:, 0] - fc[:, 1]\n",
    "\n",
    "        # --- actual Real GDP values ---\n",
    "        forecast_dates = pd.date_range(start=origin_date, periods=fh, freq=\"Q\") + pd.offsets.QuarterEnd(0)\n",
    "        actual_real_gdp = (\n",
    "            df_q[\"GDP\"].reindex(forecast_dates, method=\"nearest\") -\n",
    "            df_q[\"GDPDEF\"].reindex(forecast_dates, method=\"nearest\")\n",
    "        )\n",
    "\n",
    "        # --- store ---\n",
    "        results[start] = {\n",
    "            \"origin\": origin_date,\n",
    "            \"forecast_dates\": forecast_dates,\n",
    "            \"forecast\": real_gdp_forecast,\n",
    "            \"actual\": actual_real_gdp\n",
    "        }\n",
    "\n",
    "        # --- plot each recession’s forecast ---\n",
    "        ax.plot(forecast_dates, real_gdp_forecast, \"--\", lw=1.2, alpha=0.8, label=f\"Forecast {origin_date.year}\")\n",
    "        ax.plot(forecast_dates, actual_real_gdp, \"k\", lw=0.8, alpha=0.5)\n",
    "\n",
    "    # --- highlight the 2008 crisis in red ---\n",
    "    if pd.Timestamp(\"2008-01-01\") in results:\n",
    "        res_2008 = results[pd.Timestamp(\"2008-01-01\")]\n",
    "        ax.plot(res_2008[\"forecast_dates\"], res_2008[\"forecast\"], \"r\", lw=2.2, label=\"2008 Forecast\")\n",
    "\n",
    "    ax.set_title(\"Real GDP Forecasts Before Recessions (12-Quarter Horizon)\")\n",
    "    ax.set_ylabel(\"Normalized Real GDP (Scaled)\")\n",
    "    ax.legend(frameon=False, loc=\"best\", ncol=2)\n",
    "    ax.grid(True, alpha=0.3)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    return results\n"
   ]
  },
