   "cell_type": "code",
   "execution_count": 137,
   "id": "4bc70683",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def rolling_forecast_analysis(full_data, r=4, P=2, fh=4,\n",
    "                              start_origin=\"1976-12-31\",\n",
    "                              end_sample=\"2020-12-31\"):\n",
    "    \"\"\"\n",
    "    Iterative out-of-sample VECM forecasts and real-GDP accuracy plots.\n",
    "    \n",
    "    full_data : DataFrame\n",
    "        The transformed dataset returned by VECMAnalyzer.transform_data() (includes USREC).\n",
    "    r : int\n",
    "        Cointegration rank.\n",
    "    P : int\n",
    "        Lag order in levels (so k_ar_diff = P - 1).\n",
    "    fh : int\n",
    "        Forecast horizon (quarters ahead).\n",
    "    start_origin : str or Timestamp\n",
    "        Start of out-of-sample forecast window (default 1976Q4).\n",
    "    end_sample : str or Timestamp\n",
    "        Last quarter to include in rolling experiment.\n",
    "    \"\"\"\n",
    "    # --- select macroeconomic variables used in the model ---\n",
    "    cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]\n",
    "    df = full_data[cols].copy().dropna().loc[:end_sample]\n",
    "    df = (df - df.mean()) / df.std()  # same normalization as before\n",
    "\n",
    "    n_series = df.shape[1]\n",
    "    T0 = pd.Timestamp(start_origin)\n",
    "    all_dates = df.index\n",
    "\n",
    "    # find index of initial origin\n",
    "    try:\n",
    "        start_idx = all_dates.get_loc(T0)\n",
    "    except KeyError:\n",
    "        # if not exact match, find nearest date after\n",
    "        start_idx = all_dates.get_indexer([T0], method=\"nearest\")[0]\n",
    "\n",
    "    # number of forecast origins we can iterate through\n",
    "    n_forecasts = len(all_dates[start_idx:]) - fh\n",
    "    if n_forecasts <= 0:\n",
    "        raise ValueError(\"Not enough data beyond forecast origin to roll forward.\")\n",
    "\n",
    "    y_forecast = np.full((n_forecasts, fh, n_series), np.nan)\n",
    "\n",
    "    print(f\"Rolling forecasts from {all_dates[start_idx]} forward ({n_forecasts} origins)\")\n",
    "\n",
    "    # --- main rolling loop ---\n",
    "    for t in range(n_forecasts):\n",
    "        sample = df.iloc[: start_idx + t + 1]\n",
    "        vecm = VECM(sample, k_ar_diff=P - 1, coint_rank=r, deterministic=\"co\").fit()\n",
    "        fc = vecm.predict(steps=fh)\n",
    "        y_forecast[t, :, :] = fc\n",
    "\n",
    "    # --- organize forecast metadata ---\n",
    "    origin_dates = all_dates[start_idx:start_idx + n_forecasts]\n",
    "    forecast_dates = pd.DataFrame({\n",
    "        f\"h{k+1}\": origin_dates + pd.offsets.QuarterEnd(k+1)\n",
    "        for k in range(fh)\n",
    "    }, index=origin_dates)\n",
    "\n",
    "    forecasts = {\n",
    "        col: pd.DataFrame(y_forecast[:, :, j], index=origin_dates,\n",
    "                          columns=[f\"h{k+1}\" for k in range(fh)])\n",
    "        for j, col in enumerate(cols)\n",
    "    }\n",
    "\n",
    "    # --- compute Real GDP forecast and actual values ---\n",
    "    realGDP_forecast = forecasts[\"GDP\"][\"h4\"] - forecasts[\"GDPDEF\"][\"h4\"]\n",
    "    realGDP_actual = (\n",
    "        df[\"GDP\"].reindex(forecast_dates[\"h4\"].values).values -\n",
    "        df[\"GDPDEF\"].reindex(forecast_dates[\"h4\"].values).values\n",
    "    )\n",
    "\n",
    "    # --- plot results ---\n",
    "    fig, axes = plt.subplots(2, 1, figsize=(9, 6), sharex=True)\n",
    "    axes[0].plot(forecast_dates[\"h4\"], realGDP_forecast, \"r\", label=\"Forecast\")\n",
    "    axes[0].plot(forecast_dates[\"h4\"], realGDP_actual, \"b\", label=\"Actual\")\n",
    "    axes[0].set_title(\"Real GDP vs. Forecast (4-Quarters Ahead)\")\n",
    "    axes[0].set_ylabel(\"Scaled Log Level\")\n",
    "    axes[0].legend(frameon=False)\n",
    "\n",
    "    axes[1].plot(forecast_dates[\"h4\"], realGDP_forecast - realGDP_actual, \"k\")\n",
    "    axes[1].set_title(\"Real GDP Forecast Error (4-Quarters Ahead)\")\n",
    "    axes[1].set_ylabel(\"Error (Forecast - Actual)\")\n",
    "\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    return forecasts, forecast_dates\n"
   ]
  },
