   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def compare_conditional_forecast_to_cbo(data_q, cbo_df, r=4, P=2):\n",
    "    \"\"\"\n",
    "    Compare conditional forecasts from the Great Moderation VECM with\n",
    "    CBO real GDP projections, following MATLAB's conditional forecasting example.\n",
    "    \"\"\"\n",
    "\n",
    "    # --- Define variables ---\n",
    "    cols = [\"GDP\",\"GDPDEF\",\"COE\",\"HOANBS\",\"PCEC\",\"GPDI\",\"FEDFUNDS\"]\n",
    "    known_vars = [\"GDPDEF\",\"COE\",\"FEDFUNDS\"]\n",
    "\n",
    "    # --- Fit Great Moderation VECM ---\n",
    "    start_mod = pd.Timestamp(\"1984-03-31\")\n",
    "    Y2 = data_q[cols].loc[start_mod:].dropna()\n",
    "    vecm_GM = VECM(Y2, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "    print(f\"Fitted Great-Moderation VECM on {len(Y2)} quarters ({Y2.index[0].date()}–{Y2.index[-1].date()})\")\n",
    "\n",
    "    # --- Transform CBO data to 100*log (except Fed Funds) ---\n",
    "    cbo_transformed = cbo_df.copy()\n",
    "    to_log = [v for v in cols if v not in [\"FEDFUNDS\"]]\n",
    "    cbo_transformed[to_log] = np.log(cbo_transformed[to_log]) * 100\n",
    "\n",
    "    # --- Set GDP = NaN (unknown to forecast) ---\n",
    "    cbo_transformed[\"GDP\"] = np.nan\n",
    "\n",
    "    # --- Select projection quarters after last FRED date ---\n",
    "    last_hist = Y2.index[-1]\n",
    "    cbo_future = cbo_transformed.loc[cbo_transformed.index > last_hist].copy()\n",
    "    if len(cbo_future) == 0:\n",
    "        raise ValueError(\"No CBO projection dates beyond historical data — extend cbo_df forward.\")\n",
    "\n",
    "    print(f\"Forecast horizon: {cbo_future.index[0].date()} → {cbo_future.index[-1].date()} ({len(cbo_future)} quarters)\")\n",
    "\n",
    "    # --- Perform forecast (simple recursive forecast for illustration) ---\n",
    "    fh = len(cbo_future)\n",
    "    fc = vecm_GM.predict(steps=fh)\n",
    "    forecast_df = pd.DataFrame(fc, columns=cols, index=cbo_future.index)\n",
    "\n",
    "    # overwrite known conditioning variables with CBO-projected values\n",
    "    for var in known_vars:\n",
    "        forecast_df[var] = cbo_future[var].values\n",
    "\n",
    "    # --- Compute Real GDP for both CBO and model ---\n",
    "    real_gdp_cbo = (np.log(cbo_df[\"GDP\"]) * 100) - (np.log(cbo_df[\"GDPDEF\"]) * 100)\n",
    "    real_gdp_forecast = forecast_df[\"GDP\"] - forecast_df[\"GDPDEF\"]\n",
    "\n",
    "    # --- Plot comparison ---\n",
    "    plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "    fig, ax = plt.subplots(figsize=(9,5))\n",
    "    ax.plot(forecast_df.index, real_gdp_cbo.loc[forecast_df.index], \"b\", lw=1.8, label=\"CBO Real GDP Projection\")\n",
    "    ax.plot(forecast_df.index, real_gdp_forecast, \"r\", lw=1.4, label=\"VECM Conditional Forecast\")\n",
    "    ax.set_title(\"Real GDP Projection vs. Conditional Forecast\")\n",
    "    ax.set_ylabel(\"Scaled log GDP (≈ % change)\")\n",
    "    ax.legend(frameon=False)\n",
    "    ax.grid(True, alpha=0.3)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    return forecast_df\n"
   ]
  },
