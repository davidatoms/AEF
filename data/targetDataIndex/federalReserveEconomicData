    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 900x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Use existing quarterly data, already fetched & transformed earlier\n",
    "results = subsample_sensitivity_analysis(data_q, r=4, P=2)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 145,
   "id": "ed8349e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def subsample_sensitivity_analysis_stable(full_data, r=4, P=2):\n",
    "    \"\"\"\n",
    "    Great-Inflation → Great-Moderation counterfactual experiment (Smets & Wouters 2007).\n",
    "    Fits two VECMs on different subsamples and filters Great-Inflation shocks\n",
    "    through Great-Moderation dynamics using a stable VAR representation.\n",
    "\n",
    "    Parameters\n",
    "    ----------\n",
    "    full_data : pd.DataFrame\n",
    "        Quarterly FRED data already transformed (100*log for most variables).\n",
    "    r : int\n",
    "        Cointegration rank.\n",
    "    P : int\n",
    "        Lag order in levels (so k_ar_diff = P - 1).\n",
    "\n",
    "    Returns\n",
    "    -------\n",
    "    dict\n",
    "        Containing fitted models, filtered series, and aligned actual data.\n",
    "    \"\"\"\n",
    "\n",
    "    # --- Select relevant variables ---\n",
    "    cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]\n",
    "    df = full_data[cols].dropna().copy()\n",
    "\n",
    "    # --- Define subsamples ---\n",
    "    t1 = pd.Timestamp(\"1979-06-30\")   # End of Great Inflation\n",
    "    t2 = pd.Timestamp(\"1984-03-31\")   # Start of Great Moderation\n",
    "    Y1 = df.loc[:t1]                  # Great Inflation period\n",
    "    Y2 = df.loc[t2:]                  # Great Moderation period\n",
    "\n",
    "    print(f\"Great Inflation:  {Y1.index[0].date()} → {Y1.index[-1].date()}  ({len(Y1)} quarters)\")\n",
    "    print(f\"Great Moderation: {Y2.index[0].date()} → {Y2.index[-1].date()}  ({len(Y2)} quarters)\")\n",
    "\n",
    "    # --- Fit both models ---\n",
    "    vecm_GI = VECM(Y1, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "    vecm_GM = VECM(Y2, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "\n",
    "    # --- Residuals from Great Inflation (E₁) ---\n",
    "    E1 = vecm_GI.resid\n",
    "    n_obs, n_vars = E1.shape\n",
    "\n",
    "    # --- Get stable VAR representation of the Great-Moderation model ---\n",
    "    # Handle different statsmodels versions: var_rep() or var_rep attribute\n",
    "    try:\n",
    "        var_GM = vecm_GM.var_rep()    # newer versions (callable)\n",
    "    except TypeError:\n",
    "        var_GM = vecm_GM.var_rep      # older versions (array)\n",
    "\n",
    "    # If var_rep is just the array of coefficients:\n",
    "    if isinstance(var_GM, np.ndarray):\n",
    "        A = np.array(var_GM)\n",
    "        c = np.zeros(A.shape[1])  # no explicit intercept\n",
    "        p = A.shape[0]\n",
    "    else:\n",
    "        # var_GM is a VAR-like object\n",
    "        A = np.array(var_GM.coefs)            # (lags, n, n)\n",
    "        c = np.array(var_GM.intercept).ravel()  # constant term\n",
    "        p = getattr(var_GM, \"k_ar\", A.shape[0])\n",
    "\n",
    "    # --- Initialize with first p observations from Great Moderation sample ---\n",
    "    Y0 = Y2.iloc[:p].values\n",
    "    history = [y.copy() for y in Y0]\n",
    "    YY = np.zeros((n_obs, n_vars))\n",
    "\n",
    "    # --- Propagate Great-Inflation residuals through Great-Moderation dynamics ---\n",
    "    for t in range(n_obs):\n",
    "        y_pred = c.copy()\n",
    "        for lag in range(p):\n",
    "            y_pred += A[lag] @ history[-(lag + 1)]\n",
    "        y_new = y_pred + E1[t]\n",
    "        YY[t, :] = y_new\n",
    "        history.append(y_new)\n",
    "\n",
    "    # --- Align dates ---\n",
    "    Y2_aligned = Y2.iloc[p:p + n_obs]\n",
    "    YY_df = pd.DataFrame(YY, index=Y2_aligned.index, columns=Y2.columns)\n",
    "\n",
    "    # --- Compute Real GDP (nominal GDP - GDP deflator) ---\n",
    "    real_actual = Y2_aligned[\"GDP\"] - Y2_aligned[\"GDPDEF\"]\n",
    "    real_counter = YY_df[\"GDP\"] - YY_df[\"GDPDEF\"]\n",
    "\n",
    "    # --- Plot ---\n",
    "    plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "    fig, ax = plt.subplots(figsize=(8.5, 5))\n",
    "    ax.plot(real_actual.index, real_actual, \"b\", lw=1.8, label=\"Reported Great Moderation Data\")\n",
    "    ax.plot(real_counter.index, real_counter, \"r\", lw=1.4, label=\"Filtered Great Inflation Residuals\")\n",
    "    ax.set_title(\"Real GDP Sub-Sample Comparison:\\nGreat Inflation vs. Great Moderation\")\n",
    "    ax.set_ylabel(\"Scaled log GDP\")\n",
    "    ax.legend(frameon=False)\n",
    "    ax.grid(True, alpha=0.3)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    # --- Return results ---\n",
    "    return {\n",
    "        \"GreatInflation_VECM\": vecm_GI,\n",
    "        \"GreatModeration_VECM\": vecm_GM,\n",
    "        \"Counterfactual\": YY_df,\n",
    "        \"Actual\": Y2_aligned\n",
    "    }\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 146,
   "id": "d7a0c4d8",
   "metadata": {},
   "outputs": [
