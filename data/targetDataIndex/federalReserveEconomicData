    "from fredapi import Fred\n",
    "import pandas as pd\n",
    "\n",
    "# --- initialize FRED connection ---\n",
    "FRED_API_KEY = \"5e5d96a6be5ff1469e7da93e5fb148b7\"\n",
    "fred = Fred(api_key=FRED_API_KEY)\n",
    "\n",
    "# --- specify series you want ---\n",
    "series_ids = {\n",
    "    \"GDP\": \"GDP\",           # Nominal GDP (Billions USD, SAAR)\n",
    "    \"GDPDEF\": \"GDPDEF\",     # GDP Implicit Price Deflator (Index 2012=100)\n",
    "    \"COE\": \"COE\",           # Compensation of Employees (Billions USD)\n",
    "    \"FEDFUNDS\": \"FEDFUNDS\"  # Federal Funds Rate (Percent)\n",
    "}\n",
    "\n",
    "# --- fetch and resample to quarter-end ---\n",
    "raw = {}  # initialize dictionary before loop\n",
    "\n",
    "for name, sid in series_ids.items():\n",
    "    s = fred.get_series(sid)\n",
    "    s = s.resample(\"QE\").last()   # quarterly, end-of-quarter\n",
    "    raw[name] = s\n",
    "\n",
    "# --- combine into DataFrame ---\n",
    "cbo_df = pd.DataFrame(raw)\n",
    "cbo_df.index.name = \"Date\"\n",
    "\n",
    "# --- add missing conditional variables (placeholders) ---\n",
    "for col in [\"HOANBS\", \"PCEC\", \"GPDI\"]:\n",
    "    cbo_df[col] = np.nan\n",
    "\n",
    "# --- reorder to match model variable order ---\n",
    "cbo_df = cbo_df[[\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]]\n",
    "\n",
    "# --- preview last few quarters ---\n",
    "print(cbo_df.tail())"
   ]
