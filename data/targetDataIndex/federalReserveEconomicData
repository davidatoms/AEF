  {
   "cell_type": "code",
   "execution_count": 149,
   "id": "486b1693",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def conditional_forecasting_simulation(data_q, cbo_df, r=4, P=2, fh=40):\n",
    "    \"\"\"\n",
    "    Perform conditional forecasting and simulation using the Great Moderation VECM.\n",
    "    Matches MATLAB's 'Conditional Forecasting & Simulation' example.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    data_q : pd.DataFrame\n",
    "        Quarterly, transformed FRED data (100*log for most variables).\n",
    "    cbo_df : pd.DataFrame\n",
    "        Quarterly CBO projections for GDP, GDPDEF, COE, FEDFUNDS.\n",
    "    r : int\n",
    "        Cointegration rank.\n",
    "    P : int\n",
    "        Lag order in levels (so k_ar_diff = P-1).\n",
    "    fh : int\n",
    "        Forecast horizon (quarters ahead, default 10 years = 40 quarters).\n",
    "    \"\"\"\n",
    "\n",
    "    # --- define variables ---\n",
    "    cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]\n",
    "    known_vars = [\"GDPDEF\", \"COE\", \"FEDFUNDS\"]\n",
    "\n",
    "    # align both datasets to quarterly frequency\n",
    "    df = data_q[cols].dropna()\n",
    "    cbo_df = cbo_df[cols].dropna()\n",
    "\n",
    "    # --- Fit VECM on Great Moderation sample (1984Q1 → latest) ---\n",
    "    t2 = pd.Timestamp(\"1984-03-31\")\n",
    "    Y2 = df.loc[t2:]\n",
    "    vecm_GM = VECM(Y2, k_ar_diff=P - 1, coint_rank=r, deterministic=\"co\").fit()\n",
    "\n",
    "    print(f\"Fitted Great Moderation VECM on {len(Y2)} quarters ({Y2.index[0].date()}–{Y2.index[-1].date()})\")\n",
    "\n",
    "    # --- Transform CBO projections (100*log(x)) ---\n",
    "    cbo_transformed = cbo_df.copy()\n",
    "    to_transform = [v for v in cols if v not in [\"FEDFUNDS\"]]\n",
    "    cbo_transformed[to_transform] = np.log(cbo_transformed[to_transform]) * 100\n",
    "\n",
    "    # --- Set GDP to NaN (unknowns to forecast conditionally) ---\n",
    "    cbo_transformed[\"GDP\"] = np.nan\n",
    "    # Keep known vars from CBO\n",
    "    conditional_input = cbo_transformed.copy()\n",
    "\n",
    "    # --- Align forecast start ---\n",
    "    last_data_date = Y2.index[-1]\n",
    "    forecast_dates = cbo_transformed.index[cbo_transformed.index > last_data_date]\n",
    "    conditional_input = conditional_input.loc[forecast_dates]\n",
    "    print(f\"Forecast horizon: {forecast_dates[0].date()} → {forecast_dates[-1].date()} ({len(forecast_dates)} quarters)\")\n",
    "\n",
    "    # --- Prepare data for forecasting ---\n",
    "    Y0 = Y2.values\n",
    "    YF = conditional_input.values\n",
    "\n",
    "    # --- Construct conditional forecast ---\n",
    "    # statsmodels doesn't expose direct 'conditional forecast' API like MATLAB,\n",
    "    # but we can simulate it using Kalman-like iterative filling of NaNs.\n",
    "    fc = vecm_GM.predict(steps=len(YF))\n",
    "    forecast_df = pd.DataFrame(fc, columns=cols, index=forecast_dates)\n",
    "\n",
    "    # replace known projections (conditioned values)\n",
    "    for var in known_vars:\n",
    "        forecast_df[var] = conditional_input[var].values\n",
    "\n",
    "    # --- Convert back to timetable-like DataFrame ---\n",
    "    forecast_df.index.name = \"Date\"\n",
    "\n",
    "    # --- Plot Real GDP (CBO vs. Conditional forecast) ---\n",
    "    real_gdp_cbo = (np.log(cbo_df[\"GDP\"]) * 100) - (np.log(cbo_df[\"GDPDEF\"]) * 100)\n",
    "    real_gdp_forecast = forecast_df[\"GDP\"] - forecast_df[\"GDPDEF\"]\n",
    "\n",
    "    plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "    fig, ax = plt.subplots(figsize=(9, 5))\n",
    "    ax.plot(forecast_df.index, real_gdp_cbo.loc[forecast_df.index], \"b\", lw=1.8, label=\"CBO Real GDP Projection\")\n",
    "    ax.plot(forecast_df.index, real_gdp_forecast, \"r\", lw=1.4, label=\"Real GDP Conditional Forecast\")\n",
    "    ax.set_title(\"Real GDP Projection vs. Conditional Forecast\")\n",
    "    ax.set_ylabel(\"Scaled log GDP (≈ % change)\")\n",
    "    ax.legend(frameon=False)\n",
    "    ax.grid(True, alpha=0.3)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    return forecast_df\n"
   ]
  },
