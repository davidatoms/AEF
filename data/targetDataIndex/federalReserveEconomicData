     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Great Inflation:  1954-09-30 → 1979-06-30  (100 quarters)\n",
      "Great Moderation: 1984-03-31 → 2025-06-30  (166 quarters)\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 850x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# subsample sensitivity analysis with stable filtering\n",
    "results = subsample_sensitivity_analysis_stable(data_q, r=4, P=2)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 147,
   "id": "c37c9423",
   "metadata": {},
   "outputs": [],
   "source": [
    "import os\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def recession_counterfactuals(full_data, r=4, P=2, save=True, outdir=\"graphs_recessions\"):\n",
    "    \"\"\"\n",
    "    Counterfactual (shock-transfer) experiment for every NBER recession in the dataset.\n",
    "    Fits pre- and post-recession VECMs and propagates pre-recession shocks\n",
    "    through post-recession dynamics (stable VAR form).\n",
    "\n",
    "    Creates both level (billions USD) and log-scaled (100×ln) plots.\n",
    "    \"\"\"\n",
    "\n",
    "    # --- ensure output directory exists ---\n",
    "    if save and not os.path.exists(outdir):\n",
    "        os.makedirs(outdir)\n",
    "\n",
    "    # --- macro variables (exclude USREC for modeling) ---\n",
    "    macro_vars = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]\n",
    "    if \"USREC\" not in full_data.columns:\n",
    "        raise ValueError(\"The input DataFrame must include a USREC column for recession identification.\")\n",
    "\n",
    "    df = full_data[macro_vars + [\"USREC\"]].dropna().copy()\n",
    "    rec = df[\"USREC\"].astype(int)\n",
    "\n",
    "    # --- detect recession start dates ---\n",
    "    starts = []\n",
    "    prev = 0\n",
    "    for date, val in rec.items():\n",
    "        if val == 1 and prev == 0:\n",
    "            starts.append(date)\n",
    "        prev = val\n",
    "    # de-duplicate close starts (<2 quarters apart)\n",
    "    clean_starts = []\n",
    "    for i, d in enumerate(starts):\n",
    "        if i == 0 or (d - starts[i - 1]).days > 120:\n",
    "            clean_starts.append(d)\n",
    "    starts = clean_starts\n",
    "\n",
    "    print(f\"Detected {len(starts)} recessions ({df.index[0].date()} – {df.index[-1].date()}):\")\n",
    "    for s in starts:\n",
    "        print(\"  \", s.date())\n",
    "\n",
    "    results = {}\n",
    "\n",
    "    for start in starts:\n",
    "        # Split data: pre- and post-recession\n",
    "        pre_df = df.loc[:start - pd.offsets.QuarterEnd(1), macro_vars]\n",
    "        post_df = df.loc[start + pd.offsets.QuarterEnd(1):, macro_vars]\n",
    "\n",
    "        if len(pre_df) < 40 or len(post_df) < 40:\n",
    "            continue\n",
    "\n",
    "        print(f\"\\nRecession starting {start.date()} ({len(pre_df)} pre, {len(post_df)} post quarters)\")\n",
    "\n",
    "        # Fit pre- and post-recession VECMs\n",
    "        vecm_pre  = VECM(pre_df,  k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "        vecm_post = VECM(post_df, k_ar_diff=P-1, coint_rank=r, deterministic=\"co\").fit()\n",
    "\n",
    "        E_pre = vecm_pre.resid\n",
    "        n_obs, n_vars = E_pre.shape\n",
    "\n",
    "        # Obtain stable VAR representation of post-recession model\n",
    "        try:\n",
    "            var_post = vecm_post.var_rep()\n",
    "        except TypeError:\n",
    "            var_post = vecm_post.var_rep\n",
    "        if isinstance(var_post, np.ndarray):\n",
    "            A = np.array(var_post)\n",
    "            c = np.zeros(A.shape[1])\n",
    "            p = A.shape[0]\n",
    "        else:\n",
    "            A = np.array(var_post.coefs)\n",
    "            c = np.array(var_post.intercept).ravel()\n",
    "            p = getattr(var_post, \"k_ar\", A.shape[0])\n",
    "\n",
    "        # Initialize with first p observations from post-recession sample\n",
    "        Y0 = post_df.iloc[:p].values\n",
    "        hist = [y.copy() for y in Y0]\n",
    "        YY = np.zeros((n_obs, n_vars))\n",
    "\n",
    "        # Filter: propagate pre-recession residuals through post-recession model\n",
    "        for t in range(n_obs):\n",
    "            y_pred = c.copy()\n",
    "            for lag in range(p):\n",
    "                y_pred += A[lag] @ hist[-(lag + 1)]\n",
    "            y_new = y_pred + E_pre[t]\n",
    "            YY[t, :] = y_new\n",
    "            hist.append(y_new)\n",
    "        YY = np.array(YY)\n",
    "\n",
    "        # Align with dates in post-recession sample\n",
    "        post_aligned = post_df.iloc[p:p + n_obs]\n",
    "        min_len = min(len(post_aligned), len(YY))\n",
    "        post_aligned = post_aligned.iloc[:min_len]\n",
    "        YY = YY[:min_len]\n",
    "        YY_df = pd.DataFrame(YY, index=post_aligned.index, columns=macro_vars)\n",
    "\n",
    "        # --- Compute both log-scaled and level Real GDP ---\n",
    "        # Log-scaled (100×ln)\n",
    "        real_actual_log  = post_aligned[\"GDP\"] - post_aligned[\"GDPDEF\"]\n",
    "        real_counter_log = YY_df[\"GDP\"] - YY_df[\"GDPDEF\"]\n",
    "\n",
    "        # Convert to levels (billions USD)\n",
    "        post_lvl = np.exp(post_aligned / 100.0)\n",
    "        YY_lvl   = np.exp(YY_df / 100.0)\n",
    "        real_actual_lvl  = post_lvl[\"GDP\"] - post_lvl[\"GDPDEF\"]\n",
    "        real_counter_lvl = YY_lvl[\"GDP\"] - YY_lvl[\"GDPDEF\"]\n",
    "\n",
    "        # --- Plot both side-by-side ---\n",
    "        plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "        fig, axes = plt.subplots(1, 2, figsize=(13, 5), sharex=False)\n",
    "\n",
    "        # Left: Levels (billions)\n",
    "        ax = axes[0]\n",
    "        ax.plot(real_actual_lvl.index, real_actual_lvl, \"b\", lw=1.8, label=\"Reported (Post-Recession)\")\n",
    "        ax.plot(real_counter_lvl.index, real_counter_lvl, \"r\", lw=1.4, label=\"Counterfactual (Pre-Recession Shocks)\")\n",
    "        ax.set_title(\"Real GDP (Levels, billions USD)\")\n",
    "        ax.set_ylabel(\"USD (billions)\")\n",
    "        ax.legend(frameon=False)\n",
    "        ax.grid(True, alpha=0.3)\n",
    "\n",
    "        # Right: Log-scaled\n",
    "        ax = axes[1]\n",
    "        ax.plot(real_actual_log.index, real_actual_log, \"b\", lw=1.8, label=\"Reported (log-scaled)\")\n",
    "        ax.plot(real_counter_log.index, real_counter_log, \"r\", lw=1.4, label=\"Counterfactual (log-scaled)\")\n",
    "        ax.set_title(\"Real GDP (log-scaled, 100×ln x)\")\n",
    "        ax.legend(frameon=False)\n",
    "        ax.grid(True, alpha=0.3)\n",
    "\n",
    "        plt.suptitle(f\"Recession Counterfactual — Start: {start.date()}\", y=1.03, fontsize=12)\n",
    "        plt.tight_layout()\n",
    "\n",
    "        # Save or show\n",
    "        if save:\n",
    "            fname = f\"counterfactual_dual_{start.strftime('%YQ%q')}.png\"\n",
    "            fpath = os.path.join(outdir, fname)\n",
    "            plt.savefig(fpath, dpi=200, bbox_inches=\"tight\")\n",
    "            plt.close(fig)\n",
    "            print(f\"  → saved {fpath}\")\n",
    "        else:\n",
    "            plt.show()\n",
    "\n",
    "        results[start] = {\n",
    "            \"Pre_VECM\": vecm_pre,\n",
    "            \"Post_VECM\": vecm_post,\n",
    "            \"Counterfactual\": YY_df,\n",
    "            \"Actual\": post_aligned\n",
    "        }\n",
    "\n",
    "    return results\n"
   ]
  },
