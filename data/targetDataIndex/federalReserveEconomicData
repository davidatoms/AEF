   "source": [
    "# ---------------------------------------------------------------------\n",
    "#  Main pipeline\n",
    "# ---------------------------------------------------------------------\n",
    "def main():\n",
    "    print(\"Fetching FRED data…\")\n",
    "    fred_df = fetch_fred_data()\n",
    "    data = transform_data(fred_df)\n",
    "    print(\"Data shape:\", data.shape)\n",
    "\n",
    "    # restrict to overlapping quarterly range and drop NaNs\n",
    "    cols = [\"GDP\",\"GDPDEF\",\"COE\",\"HOANBS\",\"PCEC\",\"GPDI\",\"FEDFUNDS\"]\n",
    "    data_sub = data[cols].dropna().loc[\"1960\":\"2020\"]\n",
    "    data_sub = (data_sub - data_sub.mean()) / data_sub.std()\n",
    "\n",
    "    print(\"\\nRunning Johansen trace tests…\")\n",
    "    tHstar, tH1 = johansen_compare(data_sub)\n",
    "    print(\"\\nH* (trend):\\n\", tHstar)\n",
    "    print(\"\\nH1 (constant):\\n\", tH1)\n",
    "\n",
    "    r, P = 4, 2\n",
    "    print(\"\\nLikelihood-ratio test (H* vs H1)…\")\n",
    "    lr = lr_test_between_models(data_sub, r, P)\n",
    "    print(lr)\n",
    "\n",
    "    print(\"\\nFitting baseline VECM (H1)…\")\n",
    "    vecm_res = fit_vecm(data_sub, r, P)\n",
    "    alpha_c, gamma_c = constrain_tvalues(vecm_res)\n",
    "    print(\"Adjustment matrix (alpha) with constraints:\\n\", alpha_c)\n",
    "    print(\"Short-run matrix (gamma) with constraints:\\n\", gamma_c)\n",
    "\n",
    "    print(\"\\nPlotting cointegrating relations…\")\n",
    "    plot_cointegrating_relations(vecm_res, data_sub)\n",
    "    print(\"\\nDone.\")\n",
    "\n",
    "\n",
    "# ---------------------------------------------------------------------\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  },
