   "cell_type": "code",
   "execution_count": 139,
   "id": "423034da",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from statsmodels.tsa.vector_ar.vecm import VECM\n",
    "\n",
    "def analyze_fiscal_crisis_2008(full_data, r=4, P=2, fh=12, origin=\"2007-12-31\"):\n",
    "    \"\"\"\n",
    "    Analyze the 2008 fiscal crisis using 12-quarter (3-year) VECM forecasts of Real GDP.\n",
    "    \n",
    "    full_data : DataFrame\n",
    "        Quarterly, transformed dataset (from VECMAnalyzer.transform_data + resample('Q')).\n",
    "    r : int\n",
    "        Cointegration rank.\n",
    "    P : int\n",
    "        Lag order in levels (so k_ar_diff = P - 1).\n",
    "    fh : int\n",
    "        Forecast horizon (in quarters, default 12 = 3 years).\n",
    "    origin : str\n",
    "        Forecast origin (end of 2007Q4).\n",
    "    \"\"\"\n",
    "\n",
    "    # --- Ensure we’re using the same columns as before ---\n",
    "    cols = [\"GDP\", \"GDPDEF\", \"COE\", \"HOANBS\", \"PCEC\", \"GPDI\", \"FEDFUNDS\"]\n",
    "    df = full_data[cols].copy().dropna()\n",
    "    df = (df - df.mean()) / df.std()\n",
    "\n",
    "    # --- Forecast origin (2007Q4) ---\n",
    "    T = pd.Timestamp(origin)\n",
    "    T_idx = df.index.get_indexer([T], method=\"nearest\")[0]\n",
    "    sample = df.iloc[:T_idx + 1]\n",
    "\n",
    "    # --- Fit VECM and produce 12-quarter forecasts ---\n",
    "    vecm = VECM(sample, k_ar_diff=P - 1, coint_rank=r, deterministic=\"co\").fit()\n",
    "    fc, y_mse = vecm.predict(steps=fh, return_conf_int=True)  # return_conf_int added in statsmodels>=0.14\n",
    "\n",
    "    # If return_conf_int is unavailable, approximate with residual covariance:\n",
    "    if isinstance(fc, tuple):  # older statsmodels compatibility\n",
    "        fc = fc[0]\n",
    "    cov_u = vecm.sigma_u\n",
    "\n",
    "    # --- Compute ±1σ bands for Real GDP (GDP - GDPDEF) ---\n",
    "    sigma = np.zeros(fh)\n",
    "    for j in range(fh):\n",
    "        sigma[j] = np.sqrt(cov_u[0, 0] - 2 * cov_u[0, 1] + cov_u[1, 1])\n",
    "\n",
    "    # Forecast dates (quarterly)\n",
    "    forecast_dates = pd.date_range(start=T, periods=fh, freq=\"Q\") + pd.offsets.QuarterEnd(0)\n",
    "\n",
    "    # --- Compute Real GDP forecast (nominal - deflator) ---\n",
    "    real_gdp_forecast = fc[:, 0] - fc[:, 1]\n",
    "\n",
    "    # --- Actual Real GDP values from FRED data ---\n",
    "    actual_real_gdp = (\n",
    "        df[\"GDP\"].reindex(forecast_dates, method=\"nearest\") -\n",
    "        df[\"GDPDEF\"].reindex(forecast_dates, method=\"nearest\")\n",
    "    )\n",
    "\n",
    "    # --- Plot ---\n",
    "    plt.style.use(\"seaborn-v0_8-colorblind\")\n",
    "    fig, ax = plt.subplots(figsize=(9, 5))\n",
    "\n",
    "    ax.plot(forecast_dates, real_gdp_forecast + sigma, \"--r\", label=\"Forecast +1σ\")\n",
    "    ax.plot(forecast_dates, real_gdp_forecast, \"r\", label=\"Forecast\")\n",
    "    ax.plot(forecast_dates, real_gdp_forecast - sigma, \"--r\", label=\"Forecast -1σ\")\n",
    "    ax.plot(forecast_dates, actual_real_gdp, \"b\", label=\"Actual\")\n",
    "\n",
    "    ax.set_title(f\"Real GDP vs. 12-Quarter Forecast: Origin = {T.date()}\")\n",
    "    ax.set_ylabel(\"Scaled Log Level\")\n",
    "    ax.legend(frameon=False, loc=\"best\")\n",
    "    ax.grid(True, alpha=0.3)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    return {\n",
    "        \"forecast_dates\": forecast_dates,\n",
    "        \"real_gdp_forecast\": real_gdp_forecast,\n",
    "        \"actual_real_gdp\": actual_real_gdp,\n",
    "        \"sigma\": sigma\n",
    "    }\n"
   ]
  },
